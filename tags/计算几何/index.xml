&lt;/span&gt;&lt;/span&gt;&lt;span style=&#34;display:flex;&#34;&gt;&lt;span&gt;    &lt;span style=&#34;color:#66d9ef&#34;&gt;friend&lt;/span&gt; type &lt;span style=&#34;color:#a6e22e&#34;&gt;det&lt;/span&gt;(&lt;span style=&#34;color:#66d9ef&#34;&gt;const&lt;/span&gt; Vec &lt;span style=&#34;color:#f92672&#34;&gt;&amp;amp;&lt;/span&gt;A, &lt;span style=&#34;color:#66d9ef&#34;&gt;const&lt;/span&gt; Vec &lt;span style=&#34;color:#f92672&#34;&gt;&amp;amp;&lt;/span&gt;B) {
